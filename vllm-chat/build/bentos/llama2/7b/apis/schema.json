{
  "name": "llama2",
  "type": "service",
  "routes": [
    {
      "name": "generate",
      "route": "/generate",
      "batchable": false,
      "input": {
        "properties": {
          "prompt": {
            "default": "Explain superconductors like I'm five years old",
            "title": "Prompt",
            "type": "string"
          },
          "max_tokens": {
            "default": 1024,
            "maximum": 1024,
            "minimum": 128,
            "title": "Max Tokens",
            "type": "integer"
          }
        },
        "title": "Input",
        "type": "object"
      },
      "output": {
        "title": "strIODescriptor",
        "type": "string",
        "is_stream": true,
        "media_type": "text/event-stream"
      }
    }
  ]
}